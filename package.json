{
  "name": "webpack-adminLTE",
  "version": "0.0.1",
  "description": "Configure the webpack much page app demo",
  "main": "index.js",
  "scripts": {
    "del": "node build/webpack.del.conf.js",
    "webpack-dev": "webpack-dev-server --config build/webpack.dev.conf.js",
    "mock": "cross-env nodemon mock/server.js NODE_ENV=development",
    "bstp-install": "cd node_modules/bootstrap && npm install",
    "compile-bstp": "cd node_modules/bootstrap && npm run css-compile-main",
    "gulp": "gulp --config gulpfile.js",
    "dll": "rimraf dist/dll && rimraf dist/manifest && webpack --config build/webpack.dll.conf.js",
    "build": "webpack --config build/webpack.prod.conf.js",
    "webpack": "concurrently \"npm run mock\" \"npm run webpack-dev\" ",
    "dev": "concurrently \"npm run del\" \"npm run gulp\" \"npm run webpack\" ",
    "start": "cross-env nodemon mock/server.js NODE_ENV=production"
  },
  "config": {
